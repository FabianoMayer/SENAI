// algoritmo sucessor de um número
programa {
  funcao inicio() {
    // Declaração da variável
    inteiro a

    // Solicitação de um número ao usuário
    escreva("Digite um número:\n ")
    leia(a)

    // Cálculo e exibição do sucessor do número
    escreva("O sucessor do número ", a, " é igual a ", a + 1, "\n")
  }
}
